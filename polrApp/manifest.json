{
  "name": "polr App",
  "version": "1.0",
  "description": "This gets a user's voter info using the polr API",
  "manifest_version": 2,
  "background": {
	  "scripts": ["background.js"],
	  "persistent": false
  },
  "permissions": ["http://*/", "activeTab", "declarativeContent"],
  "page_action": {
	  "default_popup": "popup.html"
  }
}